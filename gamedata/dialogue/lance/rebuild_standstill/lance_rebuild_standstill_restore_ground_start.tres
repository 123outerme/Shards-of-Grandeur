[sub_resource type="Resource" id="Resource_1du48"]
script = ExtResource("1_ys8uc")
lines = Array[String](["There's nothing left in the soil to grow. I'd need to gather seeds to re-plant the grass and trees.", "And, it looks like nothing is going to take root in this ground, it's still practically smoldering.", "But, I do know someone I can ask. The Prime Minister in Leavenport may have what I need to start the forest growing again."])
animation = "stand"
speakerOverride = "@"

[sub_resource type="Resource" id="Resource_gna0a"]
