[sub_resource type="Resource" id="Resource_l5ktv"]
script = ExtResource("1_ys8uc")
lines = Array[String](["I see, that's a shame.", "In any case, now that Alessia's awake we should move on to our next order of business: Restoring our home.", "Could you use some more Affinity to heal the charred grounds?"])
