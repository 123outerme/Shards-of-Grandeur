config_version=5

[application]

config/name="UvutuDX"
config/version="v0.0.4"
run/main_scene="res://gamescenes/main_game.tscn"
config/auto_accept_quit=false
config/quit_on_go_back=false
config/features=PackedStringArray("4.2", "Mobile")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
boot_splash/use_filter=false
config/icon="res://icon.svg"

[autoload]

SaveHandler="*res://scripts/save_handler.gd"
PlayerResources="*res://scripts/player/player_resources.gd"
SceneLoader="*res://scripts/scene_loader.gd"
PlayerFinder="*res://scripts/player_finder.gd"
SettingsHandler="*res://scripts/settings/settings_handler.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=960
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"

[file_customization]

folder_colors={
